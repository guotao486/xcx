/* pages/collection/collection.wxss */
page{
    min-width: 320px;
    color: #333;
    font-family: '微软雅黑','宋体',sans-serif;
    background: #f0f0f0;
    font-size: 12px;
    word-break: break-all;
}
.my_project {
    display: block;
}
.my_project ul {
    background: #fff;
}
.my_project ul li {
    border-bottom: 1px solid #e5e5e5;
    position: relative;
    padding: 10px;
    display:block;
}
.webkit-box {
    display: inherit;
}
.pic_show {
    float: left;
    width: 5rem;
    height: 4rem;
}
.mr5 {
    margin-right: 5px;
}
.pic_show image {
    width: 100%;
    height: 100%;
    padding-top:2px;
}
.info_name {
    font-size: 12px;
    color: #333;
    line-height: 12px;
    margin: 3px 0 5px 0;
    overflow: hidden;
    text-overflow: ellipsis;
    -o-text-overflow: ellipsis;
}
.account_mod .info_all {
    font-size:12px;
    margin:5px 0;
    line-height:15px;
}
.symbol {
    font-size: 13px;
    color: #f60;
}
.f_money {
    color: #f63;
    font-size: 12px;
}
.account_mod .label_l {
    color: #666;
}
.account_mod .label_r {
    color: #555;
}
.blank5 {
    height: 5px;
    line-height: 5px;
    clear: both;
    visibility: hidden;
}
.f_r {
    float: right;
    padding-left:5px;
}
.account_index .operating .pay {
    height: 30px;
    line-height: 30px;
    color: #fff;
    display: block;
    padding: 0 10px;
    font-size: 14px;
    border-radius: 3px;
}
.bg_gray{
    background: #c0c0c0;
}
.theme_color{
    background: #55acef;
}
.bg_red{
	background: #ff4800;
}
.info_group{
  line-height: 20px
}
.blank5 {
    height: 5px;
    clear: both;
    visibility: hidden;
}
.notify{
  font-size:16px;
  position:fixed;
  height:70%;
  width:100%;
  display:flex;
  flex-direction:column;
  align-items:center;
  justify-content:center;
}
.imgsss{
  height:280rpx;
  padding-left:40rpx;
  width:290rpx;
}

