/* pages/order_listdetail/order_listdetail.wxss */
page{
    min-width: 320px;
    color: #333;
    font-family: '微软雅黑','宋体',sans-serif;
    background: #f0f0f0;
    font-size: 14px;
    word-break: break-all;
}
.ul_block {
    width: 100%;
}
.ul_block ul {
    background: #fff;
    padding: 0 10px;
    border: 1px solid #e5e5e5;
    display:block;
}
.ul_block ul li {
    position: relative;
    line-height: 30px;
    padding: 5px;
    border-bottom: 1px solid #e5e5e5;
    display:-webkit-box;
}
.ul_block_list .input_lable {
    height: 30px;
    line-height: 30px;
}
.input_lable {
    color: #666;
}
.mr10 {
    margin-right: 10px;
}
.list_con a {
    display: inline-block;
    width: 100%;
    height: 100%;
}
.blank15 {
    height: 15px;
    clear: both;
    visibility: hidden;
}
.f_money {
    color: #f63;
}